package arrayList;

public class StudentTest {

	public static void main(String[] args) {
		Student student1=new Student(1001,"박종연");
		Student student2=new Student(1002,"이원규");
		Student student3=new Student(1003,"김민우");
		Book book1=new Book("배트맨","DC");
		Book book2=new Book("태백산백1","조정래");
		Book book3=new Book("태백산백2","조정래");
		Book book4=new Book("태백산백3","조정래");
		Book book5=new Book("태백산백4","조정래");
		student1.addBook(book1);
		student1.addBook(book2);
		student1.addBook(book3);
		student2.addBook(book3);
		student2.addBook(book4);
		student3.addBook(book1);
		student3.addBook(book3);
		student3.addBook(book5);
		

	}

}
